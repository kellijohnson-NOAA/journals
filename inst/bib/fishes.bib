  journal =      j-FISHES,
  volume =       "9",
  number =       "6",
  pages =        "214",
  day =          "5",
  month =        jun,
  year =         "2024",
  CODEN =        "????",
  DOI =          "https://doi.org/10.3390/fishes9060214",
