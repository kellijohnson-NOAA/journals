  volume =       "9",
  number =       "4",
  pages =        "132",
  day =          "9",
  month =        apr,
  year =         "2024",
  CODEN =        "????",
  DOI =          "https://doi.org/10.3390/fishes9040132",
  ISSN =         "2410-3888",
