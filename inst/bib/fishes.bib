  journal-URL =  "https://www.mdpi.com/journal/fishes",
}

@Article{Imsland:2024:EDF,
  author =       "Albert Kjartan Dagbjartarson Imsland and Hj{\"o}rtur
                 Meth{\'u}salemsson",
