  year =         "2023",
  CODEN =        "????",
  DOI =          "https://doi.org/10.3390/fishes8040210",
  ISSN =         "2410-3888",
  bibdate =      "Thu Sep 14 11:07:41 MDT 2023",
  bibsource =    "https://www.math.utah.edu/pub/tex/bib/fishes.bib",
