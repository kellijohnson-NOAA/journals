  journal =      j-FISHES,
  volume =       "8",
  number =       "1",
  pages =        "31",
  day =          "03",
  month =        jan,
  year =         "2023",
  CODEN =        "????",
  DOI =          "https://doi.org/10.3390/fishes8010031",
