  title =        "Correlations between Environmental Factors and the
                 Distribution of Juvenile \bioname{Hucho bleekeri} in
                 the \geoname{Taibai River, Shaanxi, China}",
  journal =      j-FISHES,
  volume =       "8",
  number =       "7",
  pages =        "379",
  day =          "21",
  month =        jul,
  year =         "2023",
  CODEN =        "????",
